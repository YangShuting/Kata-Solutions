Array.prototype.all = function (p) {
  // TODO: Implement this function
  if( this.length != 0 ){
  for (var i=0; i < this.length; i++) {
  	if (i == ((this.length)-1)) {
  		if(p(this[i])) {
  			return true;
  		} else {
  		    return false;
  		}
  	}
  	else if(p(this[i])) {
  		continue;
  	} else {
  		return false;
  	}
  }
  return null;
}
else{
  return true;
}




Array.prototype.none = function (p) {
  // TODO: Implement this function
  if( this.length != 0 ){
     for (var i=0; i < this.length; i++) {
  	if (i == ((this.length)-1)) {
  		if(p(this[i])) {
  			return false;
  		} else {
  		    return true;
  		}
  	}
  	else if(p(this[i])) {
  		return false;
  	} else {
  		continue;
  	}
  }
  return null;
  }
 else {
  return true;
 }
};



Array.prototype.any = function (p) {
  // TODO: Implement this function
  if( this.length != 0 ){
      for (var i=0; i < this.length; i++) {
  	if (i == ((this.length)-1)) {
  		if(p(this[i])) {
  			return true;
  		} else {
  		    return false;
  		}
  	}
  	else if(p(this[i])) {
  		return true;
  	} else {
  		continue;
  	}
  }
  return null;
  }
  else{
    return false;
  }
};


